<Window x:Class="SkillboxHomework10_1.AccessWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SkillboxHomework10_1"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen"
        Title="AccessWindow" Height="200" Width="250">
    <Grid>
        <ComboBox Name="cbAccess" Margin="0,41,0,0" VerticalAlignment="Top" Width="130" >
            <TextBlock >Консультант</TextBlock>
            <TextBlock >Менеджер</TextBlock>

        </ComboBox>
        <Button Content="Войти" HorizontalAlignment="Center" Margin="0,0,0,30" Click="Button_Click" Height="32" VerticalAlignment="Bottom"/>
        <TextBlock HorizontalAlignment="Center" Margin="0,10,0,0" TextWrapping="Wrap" Text="Должность" TextAlignment="Center" VerticalAlignment="Top" Width="140"/>

    </Grid>
</Window>
